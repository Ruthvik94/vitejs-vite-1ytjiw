{
  "name": "vite-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean:js": "rimraf ./web/webroot/_ui/responsive/common/js/react/*",
    "clean:css": "rimraf ./web/webroot/_ui/responsive/common/css/*",
    "vite:watch": "npm run clean:js && npm run clean:css && vite build --config vite.config.prod.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "grommet": "^2.35.0",
    "grommet-theme-hpe": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.8.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser-webpack-plugin": "^5.3.10",
    "vite": "^5.1.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
